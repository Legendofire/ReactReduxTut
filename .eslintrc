{
    extends : ["airbnb-base","react"],
    parserOptions: {
        ecmaVersion: 6,
        sourceType: "module",
        ecmaFeatures: {
            jsx: true
        }
    },
    rules: {
        semi: 2,
        react/jsx-uses-vars: 2,
    },
    env: {
        browser: true,
        node: true,
        es6: true,
        mongo: true
    },
}
